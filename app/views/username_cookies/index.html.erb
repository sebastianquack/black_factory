<h1>Listing username_cookies</h1>

<table>
  <tr>
    <th>Cookiehash</th>
    <th>Username</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @username_cookies.each do |username_cooky| %>
  <tr>
    <td><%= username_cooky.cookiehash %></td>
    <td><%= username_cooky.username %></td>
    <td><%= link_to 'Show', username_cooky %></td>
    <td><%= link_to 'Edit', edit_username_cooky_path(username_cooky) %></td>
    <td><%= link_to 'Destroy', username_cooky, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

